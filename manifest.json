{

  "manifest_version": 2,
  "name": "oneko",
  "version": "1.2",

  "description": "Applies the oneko.js port of xneko/oneko to all pages.",

  "icons": {
    "32": "icons/oneko-32.png",
    "48": "icons/oneko-48.png",
    "96": "icons/oneko-96.png"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["oneko.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "web_accessible_resources": [
    "oneko.gif"
  ]
}
